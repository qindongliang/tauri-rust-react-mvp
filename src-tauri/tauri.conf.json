{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "环境变量管理器",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": true
      },
      "os": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": false,
      "category": "DeveloperTool",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "identifier": "com.example.tauri-react-mvp",
      "longDescription": "A simple Tauri + React + Rust example",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "环境变量管理器",
        "width": 1200,
        "height": 800
      }
    ]
  }
}
